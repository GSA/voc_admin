<%= form_tag url_for(:action => "save_file"), :multipart => true %>
<p class="pageTitle">Add New Image</p>
  <div id="newImage">
    <p class="copyBold">
      <%= label :select_file, "Select File: "%>
      <%= file_field 'upload', 'datafile' %>
      <%= submit_tag "Upload" -%>
    </p>
  </div>
</form>

<div class="pageNav">
	<%= link_to "Cancel", images_path, :class=>"pageNav" %>
</div>
