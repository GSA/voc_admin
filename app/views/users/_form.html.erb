<%= form_for @user do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br/>
    <%= f.password_field :password%>
  </p>
  <p>
    <%= f.label :password_confirmation %><br/>
    <%= f.password_field :password_confirmation%>
  </p>
	
  <p>
    <%= f.label :f_name, "First Name"%><br />
    <%= f.text_field :f_name %>
  </p>
  <p>
    <%= f.label :l_name, "Last Name" %><br />
    <%= f.text_field :l_name %>
  </p>


  <p><%= f.submit %></p>
<% end %>
