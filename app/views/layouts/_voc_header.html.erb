<a id="skipNav" href="#skip">Skip Navigation to go to Content</a> 

<div id="header">
	<%= image_tag "voc_large_logo.png", :class=>"large_logo", :alt=>"Voice of the Consumer, Department of Health and Human Services Logo" %>
	<% if !current_user.blank? %>
		<div class="user_details">
			<p class="user"><%= current_user.email %></p>
			<%= image_tag "half_separator.png", :class=>"separator", :alt=>"separator" %>
			<%= link_to "Logout", logout_url, :class => "logout" %>
		</div>
	<% end %>
</div>
