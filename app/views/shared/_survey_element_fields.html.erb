<% object.build_survey_element if object.new_record? %>
<%= f.fields_for :survey_element do |builder| %>
	<%= builder.hidden_field :element_order, :value => survey.newest_version.next_element_number %>
	<%= builder.label :page_id %>
	<%= builder.collection_select :page_id, survey.newest_version.pages, :id, :number %>
<% end %>