<% return if @survey_version.try(:locked?) %>
<% object.build_survey_element if object.new_record? %>
<%= f.fields_for :survey_element do |builder| %>
  <% builder.object.page = @page if @page %>
  <%= builder.label :page_id, "Page: " %>
  <%= builder.collection_select :page_id, survey_version.pages.order('page_number asc'), :id, :page_number, :class => "question_page_select"%>
<% end %>
